<h1>Edit Song</h1>

<%= form_for @song do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :release_year %>
  <%= f.check_box :released %>
  <%= f.text_field :genre %>
  <%= f.text_field :artist_name %>
  <%= link_to "Delete", songs_path, method: :delete, data: { confirm: "Really?" } %>
  <%= f.submit %>
<% end %>
